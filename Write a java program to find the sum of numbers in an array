import java.util.Scanner;
public class lab4 {
public static void main(String[] args) {
Scanner input = new Scanner(System.in);

int i,j;
int sum = 0;
System.out.print("Enter the array size: ");
int n = input.nextInt();
int a[] = new int[n];
System.out.print("Enter array values: ");
for(i=0;i&lt;n;i++)
{
a[i]=input.nextInt();
sum = sum + a[i];
}
System.out.print("Sum is : "+sum);
}
}
